<GridPane alignment="CENTER" hgap="5.0" style="-fx-background-color: white;-fx-background-radius: 70px;" vgap="5.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.aj.evaidya.patreg.controller.PatRegNewController" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="13.0">
  <padding><Insets top="25" right="25" bottom="25" left="25"/></padding>
  <children>
    <VBox prefHeight="80.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
      <children>
        <Label alignment="CENTER" prefHeight="40.0" prefWidth="1000.0" styleClass="headerGridpane" textFill="#3354ff" underline="false">
          <effect>
            <Glow level="0.25" />
          </effect>
        </Label>
        <Label fx:id="statusLabel" prefHeight="35.0" prefWidth="800.0" text="" />
      </children>
    </VBox>
    <HBox alignment="CENTER" GridPane.columnIndex="0" GridPane.rowIndex="1">
      <children>
        <VBox prefHeight="75.0" prefWidth="500.0" styleClass="patRegNewVBox">
          <children>
            <Label prefHeight="30.0" style="-fx-text-fill: #ff4500" text="Name : *" />
            <TextField fx:id="nameTextField" prefHeight="40.0" prefWidth="500.0" />
          </children>
          <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
          </padding>
        </VBox>
        <VBox prefHeight="75.0" prefWidth="500.0" styleClass="patRegNewVBox">
          <children>
            <Label prefHeight="30.0" style="-fx-text-fill: #ff4500" text="Date Of Birth : *" />
            <HBox alignment="CENTER" prefHeight="40.0" prefWidth="500.0">
              <children>
                <Label alignment="CENTER_RIGHT" prefHeight="35.0" prefWidth="46.0" style="-fx-text-fill: #ff4500" text="Date">
                  <HBox.margin>
                    <Insets right="5.0" fx:id="x1" />
                  </HBox.margin>
                </Label>
                <TextField prefHeight="35.0" prefWidth="45.0" fx:id="dateTextField" />
                <Label alignment="CENTER_RIGHT" maxWidth="-Infinity" prefHeight="35.0" prefWidth="70.0" style="-fx-text-fill: #ff4500" text="Month" HBox.margin="$x1" />
                <ChoiceBox maxWidth="-Infinity" prefHeight="35.0" prefWidth="80.0" fx:id="monthChoiceBox" >
                  <items>
                    <FXCollections fx:factory="observableArrayList">
                      <String fx:value="--" />
                      <String fx:value="JAN" />
                      <String fx:value="FEB" />
                      <String fx:value="MAR" />
                      <String fx:value="APR" />
                      <String fx:value="MAY" />
                      <String fx:value="JUN" />
                      <String fx:value="JUL" />
                      <String fx:value="AUG" />
                      <String fx:value="SEP" />
                      <String fx:value="OCT" />
                      <String fx:value="NOV" />
                      <String fx:value="DEC" />
                    </FXCollections>
                  </items>
                   <value>
				        <String fx:value="--" />
				   </value>
                </ChoiceBox>
                <Label alignment="CENTER_RIGHT" prefHeight="40.0" prefWidth="60.0" style="-fx-text-fill: #ff4500" text="Year" HBox.margin="$x1" />
                <TextField prefHeight="40.0" prefWidth="50.0" fx:id="yearTextField" />
              </children>
            </HBox>
          </children>
          <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
          </padding>
        </VBox>
      </children>
    </HBox>
    <VBox prefHeight="110.0" styleClass="patRegNewVBox" GridPane.columnIndex="0" GridPane.rowIndex="2">
      <children>
        <Label prefHeight="30.0" style="-fx-text-fill: #ff4500" text="Address : *" />
        <TextField fx:id="address1TextField" prefHeight="40.0" />
        <TextField fx:id="address2TextField" prefHeight="40.0" />
      </children>
      <padding>
        <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
      </padding>
    </VBox>
    <HBox alignment="CENTER" GridPane.columnIndex="0" GridPane.rowIndex="4">
      <children>
        <VBox prefHeight="75.0" prefWidth="500.0" styleClass="patRegNewVBox">
          <children>
            <Label prefHeight="30.0" style="-fx-text-fill: #ff4500" text="State : *" />
            <ChoiceBox fx:id="stateChoiceBox" prefHeight="40.0" prefWidth="250.0" />
          </children>
          <padding>
            <Insets bottom="5.0" left="10.0" top="10.0" />
          </padding>
        </VBox>
        <VBox prefHeight="75.0" prefWidth="500.0" styleClass="patRegNewVBox">
          <children>
            <Label prefHeight="30.0" text="Pin Code :" />
            <TextField fx:id="pincodeTextField" maxWidth="-Infinity" prefHeight="35.0" prefWidth="250.0" />
          </children>
          <padding>
            <Insets bottom="5.0" left="10.0" top="10.0" />
          </padding>
        </VBox>
      </children>
    </HBox>
    <HBox alignment="CENTER" GridPane.columnIndex="0" GridPane.rowIndex="5">
      <children>
        <VBox prefHeight="75.0" prefWidth="1000.0" styleClass="patRegNewVBox">
          <children>
            <Label prefHeight="30.0" style="-fx-text-fill: #ff4500" text="Telephone : *" />
            <HBox>
              <children>
                <TextField fx:id="tel1TextField" prefHeight="40.0" prefWidth="500.0" />
                <TextField fx:id="tel2TextField" prefHeight="40.0" prefWidth="500.0" />
              </children>
            </HBox>
          </children>
          <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
          </padding>
        </VBox>
      </children>
    </HBox>
    <HBox alignment="CENTER" GridPane.columnIndex="0" GridPane.rowIndex="6">
      <AnchorPane>
        <Button alignment="CENTER" onAction="#saveAction" text="Save" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="300.0" AnchorPane.topAnchor="15.0" />
        <Button alignment="CENTER" onAction="#resetAction" text="Reset" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="300.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="15.0" />
      </AnchorPane>
    </HBox>
  </children>
  <columnConstraints>
    <ColumnConstraints prefWidth="1000.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints />
    <RowConstraints />
    <RowConstraints />
    <RowConstraints />
    <RowConstraints />
    <RowConstraints />
  </rowConstraints>
</GridPane>
